	<Localization>
		<DocLocalization Locale="de" Name="Vordefinierter Zeichnungsliniensatz" />
		<DocLocalization Locale="en" Name="Draughting Pre Defined Curve Font" />
	</Localization>
	<WhereRules>
		<DocWhereRule Name="PreDefinedCurveFontNames" UniqueId="7fd4b6e0-e2e0-451f-991b-106105188dcc">
			<Documentation>The name of the _IfcDraughtingPreDefinedCurveFont_ shall be &apos;continuous&apos;, &apos;chain&apos;, &apos;chain double dash&apos;, &apos;dashed&apos;, &apos;dotted&apos; or &apos;by layer&apos;.</Documentation>
			<Expression>SELF\IfcPredefinedItem.Name IN
       [&apos;continuous&apos;,
        &apos;chain&apos;,
        &apos;chain double dash&apos;,
        &apos;dashed&apos;,
        &apos;dotted&apos;,
        &apos;by layer&apos;]</Expression>
		</DocWhereRule>
	</WhereRules>
</DocEntity>

